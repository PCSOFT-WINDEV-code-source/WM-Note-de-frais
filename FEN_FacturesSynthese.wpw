   -
     name : actualiseSynthese
     internal_properties : CAAAAAgAAAAxXU8RIHrAh9qXj/o4kS4ljB+IM3vurZ4dWYIPkc+iZSIHNzuJDlIu1ucsdaRGMjF1MrGh+NnIFXGCeWSipxHgNlrciJ2PTUziinBxDM6YPYh/91+ncvpanq1h6q3GbKO25Ac6Or77NdcpU6j/p/JJ0qA4kA7pfMVJmRtDcQsrQvTjFRzCH/7Oz51mQKTrhyeuOMyTbzqpG+IVvPKDZ6vBZtTmSd/Sw0iPCCxAJZpabENWxMvN1rOcsIJtZsjZb3tL4N7xDAls/vBa
     procedure_id : 1383072910715518063
     type_code : 14
     code : |1+
      // Résumé : Actualise l’aperçus en synthèse
      // Syntaxe :
      // actualiseSynthese ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE actualiseSynthese()
      
      
      // Affiche le mois
      LIB_MOIS_COURANT = majuscule( ChaîneConstruit(<§$0004§>, GetMoisLibelle(gdDateCourante..Mois), gdDateCourante..Année))
      
      actualiseTotal()
      
      SAI_Total = ChaîneConstruit(<§$0002§>, NumériqueVersChaine(gmoMontantTotal, ",2fS"), pGestionValeurs.gsSymboleDeviseReference)
      SAI_Rembousements = ChaîneConstruit(<§$0003§>, NumériqueVersChaine(gmoMontantRembourse, ",2fS"), pGestionValeurs.gsSymboleDeviseReference)
      
      
      
      // Calcule le ratio pour la jauge
      Si gmoMontantRembourse <> 0 _et_ gmoMontantTotal <> 0 ALORS
      	JAUGE_Remboursements = (gmoMontantRembourse * JAUGE_Remboursements..BorneMax) / gmoMontantTotal	
      	LIB_Aucun_Remboursement..Visible = Faux
      sinon
      	JAUGE_Remboursements = 0
      	LIB_Aucun_Remboursement..Visible = Vrai
      FIN
      
     type : 458752
   -
     name : actualiseTotal
     internal_properties : 
     procedure_id : 1383462348226729389
     type_code : 14
     code : |1+
      // Résumé : Actualise le total payé
      // Syntaxe :
      //actualiseTotal ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      
      
      PROCEDURE actualiseTotal()
      
      // Variables
      dDateDebut est une Date ; dDateDebut = gdDateCourante
      dDateFin   est une Date ; dDateFin = gdDateCourante
      
      
      // Détermine début et fin du mois
      dDateDebut..Jour = 1
      dDateFin..Jour   = 31
      
      // RAZ les informations
      gmoMontantRembourse = 0
      gmoMontantTotal     = 0
      
      // Paramètre la requête
      REQ_NoteDeFraisListe.pDateDebut   = dDateDebut
      REQ_NoteDeFraisListe.pDateFin     = dDateFin
      REQ_NoteDeFraisListe.pIDEnveloppe = Null
      
      // Exécute la requête
      SI HExécuteRequête(REQ_NoteDeFraisListe) ALORS
      	
      	// Parcours la requête et alimente la ZR
      	POUR TOUT REQ_NoteDeFraisListe 
      		
      		// Cumul le montant total
      		gmoMontantTotal += MontantSelonReference(REQ_NoteDeFraisListe.Montant, REQ_NoteDeFraisListe.IDDevise,REQ_NoteDeFraisListe.TauxChange)
      		
      		// Si la note a été remboursée on la cumule sur le total des remboursées
      		SI REQ_NoteDeFraisListe.Statut = STATUT_REMBOURSEE ALORS
      			gmoMontantRembourse += MontantSelonReference(REQ_NoteDeFraisListe.Montant, REQ_NoteDeFraisListe.IDDevise,REQ_NoteDeFraisListe.TauxChange)
      		FIN	
      		
      		FIN
      	
      SINON
      	Erreur(HErreurInfo())
      FIN
