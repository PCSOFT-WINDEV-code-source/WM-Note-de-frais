  procedures :
   -
     name : factureAffiche
     internal_properties : 
     procedure_id : 1381246630420589773
     type_code : 14
     code : |1-
      // Résumé : Affiche une note de frais
      // Syntaxe :
      //factureAffiche (<nIDFacture> est entier)
      //
      // Paramètres :
      //	nIDFacture (entier) :Identifiant de la note de frais
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE factureAffiche(LOCAL nIDFacture est un entier)
      
      // Se positionne sur la note de frais
      HLitRecherchePremier(Facture,IDFacture,nIDFacture)
      SI HTrouve(Facture) ALORS
      	
      	// RAZ
      	TableauSupprimeTout(gtabCollabo)
      	
      	// Affiche les informations
      	FichierVersEcran(FEN_FactureEdition, Facture)
      	
      	SI Val(Facture.MontantTVA2) > 0  ALORS
      		gnModeCourant = MODE_TVA_SECONDAIRE
      		zmlAjouteLigne(ZM_TVA)	
      		ZM_TVA[3].SAI_TVA_SECONDAIRE = Facture.MontantTVA2
      		ZM_TVA[3].SAI_TAUX_TVA_SECONDAIRE = Facture.TauxTVA2
      		
      		// TVA unique ou plus de TVA?
      		SI Val(Facture.MontantTVA3) > 0  ALORS
      			zmlAjouteLigne(ZM_TVA)	
      			ZM_TVA[4].SAI_TVA_SECONDAIRE = Facture.MontantTVA3
      			ZM_TVA[4].SAI_TAUX_TVA_SECONDAIRE = Facture.TauxTVA3
      			//On a au max 3 TVA
      			BTN_AJOUTE_TVA..visible = faux
      		FIN
      		
      	SINON
      		gnModeCourant = MODE_TVA_UNIQUE
      		BTN_AJOUTE_TVA..ImageFond = IMG_PLUS
      		SI ZM_TVA..Occurrence > 2 ALORS
      			zmlSupprime(ZM_TVA,3)
      		FIN
      	FIN
      	
      	// Affiche la devise
      	SetDevise(Facture.IDDevise)
      	
      	// Affiche la catégorie
      	SetCategorie(Facture.IDCatégorie)
      	
      	// Client
      	SetClient(Facture.IDClient)
      	
      	// Paiement
      	SetPaiement(Facture.IDTypePaiement)
      	
      	// Projet
      	SetProjet(Facture.IDProjet)
      	
      	// Statut
      	SetStatut(Facture.Statut)
      	
      	// Enveloppe
      	SetNote(Facture.IDNoteDeFrais)
      	
      	// Photo ?
      	TableauSupprimeTout(gtabPhotos)
      	TableauSupprimeTout(gtabSupprPhotos)
      	Hfiltre(Photos,IDFacture,Facture.IDFacture)
      	HLitPremier(Photos,IDFacture)
      	TANTQUE PAS HEnDehors(Photos)
      		stUnePhoto est un stPhoto
      		stUnePhoto.nID = Photos.IDPhotos
      		stUnePhoto.sChemin = Photos.Chemin
      		Ajoute(gtabPhotos,stUnePhoto)
      		HLitSuivant(Photos,IDFacture)
      	FIN
      	HDésactiveFiltre(Photos)
      	
      	AfficheNbJustificatifs()
      	Si gtabPhotos..Occurrence > 0 ALORS
      		gnIndiceCourant = 1
      	SINON
      		gnIndiceCourant = 0	
      	FIN
      	
      	// Collaborateurs ?
      	HFiltre(Facture_Collaborateurs,IDFacture,Facture.IDFacture)
      	HLitPremier(Facture_Collaborateurs,IDFacture)
      	TANTQUE PAS HEnDehors(Facture_Collaborateurs)
      		Ajoute(gtabCollabo,Facture_Collaborateurs.IDCollaborateurs)
      		HLitSuivant(Facture_Collaborateurs)
      	FIN
      	HDésactiveFiltre(Facture_Collaborateurs)
      	
      	SetCollaborateur(gtabCollabo)
      	
      	// Possibilité de suppression
      	BTN_SUPPRIMER..Visible = Vrai
      	
      	
      	
      	sLibelléSpécial est une chaîne
      	nTypeSpécial est un entier
      	GetSpecialSelonCategorie(facture.IDCatégorie,sLibelléSpécial, nTypeSpécial)
      	// Rubrique spéciale ?
      	SI sLibelléSpécial ~= "" ALORS
      		SetSpecial(CS_AUCUNE_VALEUR)
      	SINON
      		SetSpecial(Facture.IDCatégorie, Facture.ValeurSpéciale, nTypeSpécial)	
      	FIN
      	
      	// Actualise la hauteur
      	actualiseHauteurCommentaire()
      FIN
     type : 458752
   -
     name : factureRAZ
     internal_properties : CAAAAAgAAABTHrh4nG2dg/8xlF3FzCzuK5czhtNqBFXuhRnMZLdCuKColRVAx/0vdtG4QAMptlffweZ7P7yGGXluLp1jAQpgSH8WPjgZSPS5Dp0ABrygmbxqPzT5YwIkpQZyrvAanHduijCeq4z/7PlWvitu1UnR4/QzT/6107SEiQYE8/C4igjzeB8tvd06cz2Y+0jBPLoAUx1sDGLZ48shXxj1Hl66ust6eK2izOIgFylB3eOX+WnA2CdRlzhAfyi52NWY1zUK0HKwpRuECtOBW+O27tsLofgUzGg=
     procedure_id : 1381246630420655309
     type_code : 14
     code : |1-
      // Résumé : Réinitialise une note de frais avec les valeurs par défaut
      // Syntaxe :
      //factureRAZ ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE factureRAZ()
      
      // Réinitialise les champs
      SAI_TTC = 0
      SAI_TVA     = 0
      SAI_DATE    = DateDuJour()
      
      // Valeurs par défaut
      SetCategorie(CS_AUCUNE_VALEUR)
      SetClient(CS_AUCUNE_VALEUR)
      SetPaiement(gnIDTypePaiement)
      SetProjet(CS_AUCUNE_VALEUR)
      SetStatut(STATUT_A_ENVOYER)
      SetDevise(GetDeviseDéfaut())
      SetSpecial(CS_AUCUNE_VALEUR)
      SetNote(gnIDEnveloppe)
      
      SAI_Date = poutils.getDateDéfaut()
      
      LIB_ChoixJustificatifs = chaineconstruit(<§$001c§>)
      TableauSupprimeTout(gtabPhotos)
      TableauSupprimeTout(gtabSupprPhotos)
      TableauSupprimeTout(gtabCollabo)
      SetCollaborateur(GetCollaborateursDéfaut())
      
      
      // Aucune suppression possible, c'est une nouvelle note de frais
      BTN_SUPPRIMER..Visible = Faux
      
      actualiseHauteurCommentaire()
     type : 458752
   -
     name : SetCategorie
     internal_properties : CAAAAAgAAAAKlyexebBgN4yrxvFB39nAFhvtbyErJ0MD8lDhbapcp6PqscOpVqXKS2M47z8UztEwmyrmMLOBm4BoYz4UUdCT/2PjKZkxsCzq7gSQr4PvX+SrIv87Kv6pE82MFskOozLUOO2hfzuxKjo3p+n5LPsCXO7Ej57OGsB6LbUQdXCAN9oPJeeUrD7/KgPG2ehSOiJwOggOVyN15Bf02tjCteFtmZ3Dp2QWPOAzhtLTd4OvNVMmFrlDFphwpr3dDMqO2V6DeABGnCtkjkL9CQ==
     procedure_id : 1381246630420720845
     type_code : 14
     code : |1-
      // Résumé : Positionne la catégorie
      // Syntaxe :
      //SetCategorie (<nIDCategorie>)
      //
      // Paramètres :
      // 	nIDCategorie : Identifiant de la catégorie
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE SetCategorie(LOCAL nIDCategorie)
      
      gnIDCategorie = nIDCategorie
      
      // Valeur existante ?
