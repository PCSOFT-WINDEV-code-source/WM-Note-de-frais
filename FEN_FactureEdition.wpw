   -
     name : SetSpecial
     internal_properties : 
     procedure_id : 1381246630421310669
     type_code : 14
     code : |1+
      // Résumé : Affiche / Enlève le champ spécial
      // Syntaxe :
      //SetSpecial (<nIDCategorie> [, <sValeur> [, <nTypeSpécial>]])
      //
      // Paramètres :
      //	nIDCategorie : ID Catégorie associé
      //	sValeur (valeur par défaut="") : <indiquez ici le rôle de sValeur>
      //	nTypeSpécial (valeur par défaut=0) : <indiquez ici le rôle de ntypes>
      // Valeur de retour :
      // 	Aucune
      
      PROCEDURE SetSpecial(LOCAL nIDCategorie, LOCAL sValeur = "", LOCAL nTypeSpécial = 0)
      
      
      // Faut il cacher le champ spécial ?
      SI nIDCategorie <> CS_AUCUNE_VALEUR  ALORS
      	
      	sLibelléSpécial est une chaîne
      	GetSpecialSelonCategorie(nIDCategorie, sLibelléSpécial, nTypeSpécial)
      	
      	si sLibelléSpécial ~= "" alors 
      		//Si la ligne du spécial existait, on la supprime
      		SI  ZM_Général..Occurrence = 5 ALORS
      			zmlSupprime(ZM_Général,4)
      		FIN
      	sinon
      		
      		//Il faut ajouter le "spécial"
      		
      		//Si la ligne n'existe pas déjà, on ajoute la ligne	
      		SI  ZM_Général..Occurrence < 5 ALORS
      			zmlAjouteLigne(ZM_Général)
      		fin
      		
      		selon nTypeSpécial
      			cas TYPE_NUM 
      				ZM_Général[4].SAI_SPECIAL_NUM..Libellé = sLibelléSpécial
      				ZM_Général[4].SAI_SPECIAL_NUM = sValeur
      				ZM_Général[4].SAI_SPECIAL_NUM..Visible = vrai
      				ZM_Général[4].SAI_SPECIAL_TEXTE..Visible = faux
      				
      			AUTRE CAS
      				ZM_Général[4].SAI_SPECIAL_TEXTE..Libellé = sLibelléSpécial
      				ZM_Général[4].SAI_SPECIAL_TEXTE = sValeur
      				ZM_Général[4].SAI_SPECIAL_NUM..Visible = faux
      				ZM_Général[4].SAI_SPECIAL_TEXTE..Visible = vrai
      				
      		FIN
      		
      		
      		
      	FIN
      sinon
      	//Si la ligne du spécial existait, on la supprime
      	SI  ZM_Général..Occurrence = 5 ALORS
      		zmlSupprime(ZM_Général,4)
      	FIN
      FIN
      
