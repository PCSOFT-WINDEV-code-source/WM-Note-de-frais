     code : |1+
      // Résumé : Ajoute une photo
      // Syntaxe :
      //PhotoAjoute (<sCheminPhoto>)
      //
      // Paramètres :
      //	sCheminPhoto : Chemin vers la photo
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE PhotoAjoute(LOCAL sCheminPhoto)
      
      stUnePhoto est une stPhoto
      stUnePhoto.nID = CS_AUCUNE_VALEUR
      stUnePhoto.sChemin = sCheminPhoto
      
      // On ajoute une nouvelle photo au tableau
      Ajoute(gtabPhotos,stUnePhoto)
      gnIndiceCourant = gtabPhotos..Occurrence
      
      AfficheNbJustificatifs()
     type : 458752
   -
     name : PhotoTraitement
     internal_properties : 
     procedure_id : 1381246630421507277
     type_code : 14
     code : |1-
      // Résumé : Traite les actions à faire sur les photos
      // Syntaxe :
      //PhotoTraitement (<bSauvegarder> est booléen)
      //
      // Paramètres :
      //	bSauvegarder (booléen) : Sauvegarde ?
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE PhotoTraitement(bSauvegarder est un booléen)
      
      // On annule ?
      Si bSauvegarder = Faux ALORS
      	
      	// Il faut supprimer les fichiers qui ne vont pas être utilisés
      	POUR TOUTE ELEMENT stUnePhoto de gtabPhotos
      		HLitRecherchePremier(Photos,Chemin,stUnePhoto.sChemin)
      		SI HTrouve(Photos) = Faux ALORS
      			fSupprime(stUnePhoto.sChemin)
      		FIN
      	FIN
      	
      	POUR TOUTE ELEMENT sFichier de gtabSupprPhotos
      		HLitRecherchePremier(Photos,Chemin,sFichier)
      		SI HTrouve(Photos) = Faux ALORS
      			fSupprime(sFichier)
      		FIN	
      	FIN
      SINON
      	// Supprime les photos en base qui ne sont plus souhaitées
      	POUR TOUT ELEMENT sFichier de gtabSupprPhotos
      		HLitRecherchePremier(Photos,Chemin,sFichier)
      		SI HTrouve(Photos) = Vrai ALORS
      			Si Photos.IDFacture = gnIDNoteDeFrais ALORS
      				fSupprime(sFichier)
      			FIN
      		FIN	
      	FIN
      	
      	// Ajouter les nouvelles
      	POUR TOUT ELEMENT stUnePhoto de gtabPhotos
      		
      		HLitRecherchePremier(Photos,Chemin,stUnePhoto.sChemin)
      		SI HTrouve(Photos) = Faux ALORS
      			HRAZ(Photos)
      			Photos.Chemin  = stUnePhoto.sChemin
      			photos.IDFacture = gnIDNoteDeFrais
      			Hajoute(Photos)
      		FIN
      	FIN
      FIN
     type : 458752
