     code : |1+
      // Résumé : Actualise la hauteur du champ commentaire
      // Syntaxe :
      // actualiseHauteurCommentaire ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE actualiseHauteurCommentaire()
      
      SI BTN_SUPPRIMER..Visible ALORS
      	SAI_COMMENTAIRES..Hauteur = SAI_COMMENTAIRES..HauteurInitiale
      SINON
      	// On ajoute la hauteur du bouton
      	SAI_COMMENTAIRES..Hauteur = SAI_COMMENTAIRES..HauteurInitiale + BTN_SUPPRIMER..Hauteur + 6
      FIN
      
