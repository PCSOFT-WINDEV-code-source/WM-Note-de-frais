   -
     name : factureSauvegarde
     internal_properties : 
     procedure_id : 1381246630421114061
     type_code : 14
     code : |1-
      // Résumé : Sauvegarde la note de frais
      // Syntaxe :
      //factureSauvegarde ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE factureSauvegarde()
      
      // Nouvelle note de frais ?
      SI gnIDNoteDeFrais = -1 ALORS
      	HRAZ(Facture)
      SINON
      	HLitRecherchePremier(Facture,IDFacture,gnIDNoteDeFrais)
      	SI HTrouve(Facture) = Faux ALORS
      		Erreur(<§$000c§>)
      		Ferme()
      	FIN
      FIN
      
      // Renseigne les valeurs
      EcranVersFichier(FEN_FactureEdition,Facture)
      
      
      Facture.IDCatégorie    = gnIDCategorie
      Facture.IDClient       = gnIDClient
      Facture.IDDevise       = gnIDDevise
      setdevisedefaut(gnIDDevise)
      
      
      Facture.IDProjet       = gnIDProjet
      Facture.MontantTVA1    = arrondi(SAI_TVA,2)
      Facture.Statut         = gnValeurStatut
      Facture.TauxTVA1       = SAI_TAUX_TVA
      Facture.IDTypePaiement = gnIDTypePaiement
      Facture.IDNoteDeFrais    = gnIDEnveloppe
      
      // Mode de TVA ?
      SI gnModeCourant = MODE_TVA_UNIQUE ALORS
      	Facture.MontantTVA2    = 0
      	Facture.TauxTVA2       = 0
      	Facture.MontantTVA3    = 0
      	Facture.TauxTVA3      = 0
      SINON
      	Facture.MontantTVA2    = arrondi(ZM_TVA[3].SAI_TVA_SECONDAIRE,2)
      	Facture.TauxTVA2       = ZM_TVA[3].SAI_TAUX_TVA_SECONDAIRE	
      	si ZM_TVA..Occurrence = 4 ALORS
      		Facture.MontantTVA3    = arrondi(ZM_TVA[4].SAI_TVA_SECONDAIRE,2)
      		Facture.TauxTVA3      = ZM_TVA[4].SAI_TAUX_TVA_SECONDAIRE	
      	sinon
      		Facture.MontantTVA3    = 0
      		Facture.TauxTVA3      = 0
      	FIN
      FIN
      
      si ZM_Devise..Occurrence < 2 ALORS
      	Facture.TauxChange = 0
      	Facture.FraisBancaire = 0
      sinon
      	Facture.TauxChange = ZM_Devise[2].SAI_Sup
      	Facture.FraisBancaire =  ZM_Devise[3].SAI_Sup
      FIN
      
      // Rubrique spéciale ?
      SI ZM_Général..Occurrence < 5 ALORS
      	Facture.ValeurSpéciale = ""
      SINON
      	si ZM_Général[4].SAI_SPECIAL_NUM..Visible = vrai alors 
      		Facture.ValeurSpéciale = ZM_Général[4].SAI_SPECIAL_NUM
      	FIN
      	
      	SI ZM_Général[4].SAI_SPECIAL_texte..Visible = Vrai ALORS 
      		Facture.ValeurSpéciale = SansEspace(ZM_Général[4].SAI_SPECIAL_texte)	
      	FIN
      FIN
      
      // Ajoute ou modifie
      SI gnIDNoteDeFrais <> -1 ALORS
      	HModifie(Facture)
      SINON
      	Hajoute(Facture)
      FIN
      
      // Sauvegarde l'ID
      gnIDNoteDeFrais = Facture.IDFacture
      
      // Traitement sur les photos
      PhotoTraitement(Vrai)
      
      // Actualise la période de l'enveloppe
      SI Facture.IDNoteDeFrais <> CS_AUCUNE_VALEUR ALORS
      	EnveloppeActualisePeriode(Facture.IDNoteDeFrais)
      FIN
      
      // Sauvegarde les collaborateurs
      HLitRechercheDernier(Facture_Collaborateurs,IDFacture,gnIDNoteDeFrais)
      TANTQUE HTrouve(Facture_Collaborateurs)
      	HSupprime(Facture_Collaborateurs)
      	HLitPrécédent(Facture_Collaborateurs,IDFacture)
      FIN
      
      POUR TOUT ELEMENT nIDCollaborateur de gtabCollabo
      	HRAz(Facture_Collaborateurs)
      	Facture_Collaborateurs.IDCollaborateurs = nIDCollaborateur
      	Facture_Collaborateurs.IDFacture = gnIDNoteDeFrais
      	HAjoute(Facture_Collaborateurs)
      FIN
