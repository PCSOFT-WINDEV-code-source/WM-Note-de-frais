     name : SetCollaborateur
     internal_properties : 
     procedure_id : 1381246630421572813
     type_code : 14
     code : |1-
      // Résumé : Positionne les collaborateurs
      // Syntaxe :
      //SetCollaborateur (<tabIDCollaborateur> est tableau)
      //
      // Paramètres :
      //	tabIDCollaborateur (tableau) : Tableau d'identifiant des collaborateurs
      // Valeur de retour :
      // 	Aucune
      //
      // Exemple :
      // Indiquez ici un exemple d'utilisation.
      //
      PROCEDURE SetCollaborateur(LOCAl tabIDCollaborateur est un tableau de entier)
      
      // Variable
      sNom est une chaine
      
      // Sauvegarde le tableau
      gtabCollabo = tabIDCollaborateur
      
      // Affiche les collaborateurs
      POUR TOUT ELEMENT nIDCollaborateur DE gtabCollabo
      	HLitRecherchePremier(Collaborateurs,IDCollaborateurs,nIDCollaborateur)
      	SI HTrouve(Collaborateurs) ALORS
      		sNom += [", "] + Collaborateurs.Nom	
      	FIN
      FIN
      
      lib_choixCollaborateur = sNom
