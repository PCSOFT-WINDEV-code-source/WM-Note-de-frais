     code : |1-
      // Résumé : Associe une enveloppe à la note de frais
      // Syntaxe :
      //SetNote (<nIDNote>)
      //
      // Paramètres :
      //	nIDNote : ID Enveloppe
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE SetNote(LOCAL nIDNote)
      
      gnIDEnveloppe = nIDNote
      
      // Valeur existante ?
      SI nIDNote <> CS_AUCUNE_VALEUR ALORS
      	HLitRecherchePremier(NoteFrais,IDNoteDeFrais,nIDNote)
      	SI HTrouve(NoteFrais) ALORS
      		lib_choixnote = NoteFrais.Nom
      	SINON
      		LIB_ChoixNote = ""	
      	FIN
      SINON
      	LIB_ChoixNote = ""
      FIN
