     code : |1+
      // Résumé : <indiquez ici ce que fait la procédure>
      // Syntaxe :
      //SélectionPhotoDansAlbum (<sCheminImage>)
      //
      // Paramètres :
      // 	sCheminImage : <indiquez ici le rôle de sCheminImage>
      // Valeur de retour :
      // 	Aucune
      
      PROCEDURE SélectionPhotoDansAlbum(sCheminImage)
      
      sPhoto est une chaîne = sCheminImage
      
      si fExtraitChemin(sPhoto,fExtension) ~= ".jpg" ALORS
      	
      	SI SansEspace(sPhoto) <> "" ALORS
      		
      		PhotoAjoute(sPhoto)
      		
      	FIN	
      	
      sinon
      	info(<§$0019§>)
      	retour	
      FIN
      
      
      
      
      
      
      
      
      
