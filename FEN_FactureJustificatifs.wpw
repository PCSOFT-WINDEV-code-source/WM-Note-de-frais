  procedures :
   -
     name : PhotoSupprime
     internal_properties : 
     procedure_id : 1383072244993793375
     type_code : 14
     code : |1+
      // Résumé : Supprime la photo
      // Syntaxe :
      //PhotoSupprime (<nIndice>)
      //
      // Paramètres :
      //	nIndice : Chemin vers la photo
      // Valeur de retour :
      // 	Aucune
      //
      
      PROCEDURE PhotoSupprime(LOCAL nIndice)
      
      Si nIndice <> -1 ALORS
      	
      	stUnePhoto est une stPhoto
      	stUnePhoto = gtabPhotos[nIndice]
      	
      	//S'il s'agit d'une photo déjà stockée dans la base de données => Suppression dans la base de données
      	si stUnePhoto.nID <> CS_AUCUNE_VALEUR alors
      		HLitRecherchePremier(Photos,IDPhotos,stUnePhoto.nID)
      		si HTrouve(Photos) ALORS
      			//Suppression de la photo sur le disque
      			fsupprime(Photos.Chemin)
      			//Puis dans la base de donnée
      			HSupprime(Photos)
      		FIN
      	fin
      	
      	//Suppression du tableau 
      	TableauSupprime(gtabPhotos,nIndice)
      	
      	Si gnIndiceCourant = nIndice ALORS
      		Si gtabPhotos..Occurrence = 0 ALORS
      			gnIndiceCourant = 0
      		SINON
      			Si gnIndiceCourant > gtabPhotos..Occurrence ALORS
      				gnIndiceCourant = gtabPhotos..Occurrence
      			FIN
      			
      			
      		FIN	
      	SINON
      		SI gnIndiceCourant > 1 ALORS
      			gnIndiceCourant--
      			
      		FIN
      	FIN
      	
      	
      	
      FIN
     type : 458752
   -
     name : SupprimerPièceJustificative
     internal_properties : CAAAAAgAAAC/+a7lkY1/ojCbAJxBtWJJkkdvnC/YTJk3VUZ03OHJoefKCDz95kUddoXj+gg26wTtGOjoBh5rj2vawfKDTSnJAAQdSCSFHW/N1wOZ4TcDb+BzQTr5TSu/PreGcnEpBWdmkX5rSD56HtDiA1g=
     procedure_id : 1552679855015739133
     type_code : 14
     code : |1-
      // Résumé : Suppression de la pièce justificative en cours d'affichage
      PROCEDURE SupprimerPièceJustificative()
      
      
      nIndice est un entier = FIListePosition(CFI_AffichePhoto)
      PhotoSupprime(nIndice)
      
      SI gtabPhotos..Occurrence = 0 ALORS
      	Ferme()
      SINON
      	FIListeSupprime(CFI_AffichePhoto, nIndice)
      FIN
     type : 458752
