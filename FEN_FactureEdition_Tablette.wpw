   -
     name : SetDevise
     internal_properties : 
     procedure_id : 1613886137071853760
     type_code : 14
     code : |1+
      // Résumé : Positionne la devise
      // Syntaxe :
      //SetDevise (<nIDDevise>)
      //
      // Paramètres :
      //	nIDDevise :Identifiant de la devise
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE SetDevise(LOCAL nIDDevise)
      
      gnIDDevise = nIDDevise
      
      // Affiche la devise
      SI nIDDevise <> CS_AUCUNE_VALEUR ALORS
      	HLitRecherchePremier(Devise,IDDevise,nIDDevise)
      	SI HTrouve(Devise) ALORS
      		
      		LIB_ChoixDevise = Devise.Symbole	
      		SI Devise.Symbole = "$" ALORS
      			SAI_TTC..MasqueSaisie = "+9 999 999 999,99 $" + Devise.Symbole
      			SAI_TVA..MasqueSaisie = "+99 999,99 $" + Devise.Symbole
      			
      			SI gnModeCourant = MODE_TVA_SECONDAIRE ALORS
      				ZM_TVA[3].SAI_TVA_SECONDAIRE..MasqueSaisie = "+99 999,99 $" + Devise.Symbole
      				SI ZM_TVA..Occurrence = 4 ALORS
      					ZM_TVA[4].SAI_TVA_SECONDAIRE..MasqueSaisie = "+99 999,99 $" + Devise.Symbole
      				FIN
      			FIN
      			
      		SINON
      			SAI_TTC..MasqueSaisie = "+9 999 999 999,99 " + Devise.Symbole[[ A 3]]	
      			SAI_TVA..MasqueSaisie = "+99 999,99 " + Devise.Symbole
      			
      			SI gnModeCourant = MODE_TVA_SECONDAIRE ALORS
      				ZM_TVA[3].SAI_TVA_SECONDAIRE..MasqueSaisie = "+99 999,99 " + Devise.Symbole
      				SI ZM_TVA..Occurrence = 4 ALORS
      					ZM_TVA[4].SAI_TVA_SECONDAIRE..MasqueSaisie = "+99 999,99 " + Devise.Symbole
      				FIN
      			FIN
      			
      		FIN
      		
      		
      		
      		//Si la devise choisie n'est pas la devise de référence, on demande le taux de change
      		SI Devise.Reference = Faux ALORS
      			SI ZM_Devise..Occurrence = 1 ALORS
      				zmlAjouteLigne(ZM_Devise)
      				zmlAjouteLigne(ZM_Devise)
      			FIN
      			
      			
      			ZM_Devise[2].LIB_LibSup = "1 "+ pGestionValeurs.gsSymboleDeviseReference + " ="
      			SI Devise.Symbole = "$" ALORS
      				ZM_Devise[2].SAI_Sup..MasqueSaisie = "+999 999,000000000 $" + Devise.Symbole
      			SINON
      				ZM_Devise[2].SAI_Sup..MasqueSaisie = "+999 999,000000000 " + Devise.Symbole
      			FIN
      			
      			ZM_Devise[3].LIB_LibSup = "Frais"
      			
      			SI pGestionValeurs.gsSymboleDeviseReference = "$" ALORS
      				ZM_Devise[3].SAI_Sup..MasqueSaisie  = "+999 999,99 $" + pGestionValeurs.gsSymboleDeviseReference
      			SINON
      				ZM_Devise[3].SAI_Sup..MasqueSaisie  = "+999 999,99" + pGestionValeurs.gsSymboleDeviseReference
      			FIN
      			
      			//Affichage du montant en conversion dans la devise de référence
      			ZM_TVA[1]..Hauteur = 60
      			
      			SI Facture.TauxChange <> 0 ALORS
      				ZM_Devise[2].SAI_Sup = Facture.TauxChange
      			SINON
      				ZM_Devise[2].SAI_Sup = 1
      			FIN
      			LIB_Conversion = "(" + NumériqueVersChaîne(Arrondi(SAI_TTC / ZM_Devise[2].SAI_Sup, 2),",2fS") + [ESP] + pGestionValeurs.gsSymboleDeviseReference + ")"
      			LIB_Conversion..Visible = Vrai
      			
      			ZM_Devise[3].SAI_Sup = Facture.FraisBancaire
      			
      		SINON
      			SI ZM_Devise..Occurrence > 1 ALORS
      				
      				ZM_Devise[2].SAI_Sup = 0
      				ZM_Devise[3].SAI_Sup = 0
      				
      				zmlSupprime(ZM_Devise,3)
      				zmlSupprime(ZM_Devise,2)
      				
      				ZM_TVA[1]..Hauteur = ZM_TVA[2]..Hauteur
      				LIB_Conversion..Visible = Faux
      				
      			FIN
      		FIN
      	SINON
      		Erreur("Devise introuvable")
      	FIN
      SINON
      	LIB_ChoixDevise = ""
      FIN
      
