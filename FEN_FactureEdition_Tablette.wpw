      //
      // Paramètres :
      //	nIDCategorie :Identifiant de la catégorie
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE SetCategorie(LOCAL nIDCategorie)
      
      gnIDCategorie = nIDCategorie
      
      // Valeur existante ?
      SI nIDCategorie <> CS_AUCUNE_VALEUR ALORS
      	HLitRecherchePremier(Catégorie,IDCatégorie,nIDCategorie)
      	SI HTrouve(Catégorie) ALORS
