     name : factureSauvegarde
     internal_properties : 
     procedure_id : 1613886137071198400
     type_code : 14
     code : |1-
      // Résumé : Sauvegarde la note de frais
      // Syntaxe :
      //factureSauvegarde ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE factureSauvegarde()
      
      // Nouvelle note de frais ?
      SI gnIDNoteDeFrais = -1 ALORS
      	HRAZ(Facture)
      SINON
      	HLitRecherchePremier(Facture,IDFacture,gnIDNoteDeFrais)
      	SI HTrouve(Facture) = Faux ALORS
      		Erreur(<§$0000§>)
      		Ferme()
      	FIN
      FIN
      
      // Renseigne les valeurs
      EcranVersFichier(FEN_FactureEdition_Tablette,Facture)
      
      // Aucune enveloppe ?
      Si gnIDEnveloppe = CS_AUCUNE_VALEUR ALORS
      	// Enveloppe par défaut
      	gnIDEnveloppe = 1
      FIN
      
      Facture.IDCatégorie    = gnIDCategorie
      Facture.IDClient       = gnIDClient
      Facture.IDDevise       = gnIDDevise
      SetDeviseDéfaut(gnIDDevise)
      Facture.IDProjet       = gnIDProjet
      Facture.MontantTVA1    = SAI_TVA
      Facture.Statut         = gnValeurStatut
      Facture.TauxTVA1       = SAI_TAUX_TVA
      Facture.IDTypePaiement = gnIDTypePaiement
      Facture.IDNoteDeFrais    = gnIDEnveloppe
      
      
      // Mode de TVA ?
      SI gnModeCourant = MODE_TVA_UNIQUE ALORS
      	Facture.MontantTVA2    = 0
      	Facture.TauxTVA2       = 0
      SINON
      	Facture.MontantTVA2    = ZM_TVA[3].SAI_TVA_SECONDAIRE
      	Facture.TauxTVA2       = ZM_TVA[3].SAI_TAUX_TVA_SECONDAIRE	
      FIN
      
      
      SI ZM_Devise..Occurrence < 2 ALORS
      	Facture.TauxChange = 0
      	Facture.FraisBancaire = 0
      SINON
      	Facture.TauxChange = ZM_Devise[2].SAI_Sup
      	Facture.FraisBancaire =  ZM_Devise[3].SAI_Sup
      FIN
      
      // Rubrique spéciale ?
      SI ZM_Général..Occurrence < 5 ALORS
      	Facture.ValeurSpéciale = ""
      SINON
      	SI ZM_Général[4].SAI_SPECIAL_NUM..Visible = Vrai ALORS 
      		Facture.ValeurSpéciale = ZM_Général[4].SAI_SPECIAL_NUM
      	FIN
      	
      	SI ZM_Général[4].SAI_SPECIAL_TEXTE..Visible = Vrai ALORS 
      		Facture.ValeurSpéciale = SansEspace(ZM_Général[4].SAI_SPECIAL_TEXTE)	
      	FIN
      FIN
      
      // Ajoute ou modifie
      SI gnIDNoteDeFrais <> -1 ALORS
      	HModifie(Facture)
      SINON
      	Hajoute(Facture)
      FIN
      
      // Sauvegarde l'ID
      gnIDNoteDeFrais = Facture.IDFacture
      
      // Traitement sur les photos
      PhotoTraitement(Vrai)
      
      // Actualise la période de l'enveloppe
      SI Facture.IDNoteDeFrais <> CS_AUCUNE_VALEUR ALORS
      	EnveloppeActualisePeriode(Facture.IDNoteDeFrais)
      FIN
      
      // Sauvegarde les collaborateurs
      HLitRechercheDernier(Facture_Collaborateurs,IDFacture,gnIDNoteDeFrais)
      TANTQUE HTrouve(Facture_Collaborateurs)
      	HSupprime(Facture_Collaborateurs)
      	HLitPrécédent(Facture_Collaborateurs,IDFacture)
      FIN
      
      POUR TOUT ELEMENT nIDCollaborateur de gtabCollabo
      	HRAz(Facture_Collaborateurs)
      	Facture_Collaborateurs.IDCollaborateurs = nIDCollaborateur
      	Facture_Collaborateurs.IDFacture = gnIDNoteDeFrais
      	HAjoute(Facture_Collaborateurs)
      FIN
     type : 458752
   -
     name : factureVerifie
     internal_properties : 
     procedure_id : 1613886137071263936
     type_code : 14
     code : |1+
      // Résumé : Vérifie la saisie de la note de frais
      // Syntaxe :
      //[ <Résultat> = ] factureVerifie ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	booléen : // 	Aucune
      //
      PROCEDURE factureVerifie()
      
      
      
      // Vérifie la catégorie
      SI gnIDCategorie = CS_AUCUNE_VALEUR ALORS
      	Erreur(<§$0002§>)
      	RENVOYER Faux
      FIN
      
      // Montant ?
      SI SAI_TTC = 0 ALORS
      	Erreur(<§$0001§>)
      	DonneFocus(SAI_TTC)
      	RENVOYER Faux
      FIN
      
      // Type de paiement
      SI gnIDTypePaiement = CS_AUCUNE_VALEUR ALORS
      	Erreur(<§$0003§>)
      	RENVOYER Faux
      FIN
      
      //SI gnIDEnveloppe = CS_AUCUNE_VALEUR ALORS
      //	Erreur(<§$000b§>)
      //	RENVOYER Faux
      //FIN
      
      RENVOYER Vrai
      
     type : 458752
   -
     name : actualiseSelonTVA
     internal_properties : CAAAAAgAAAA1shCPZDv2H19ICWf7R6GY7ApJ641U0r6jWiQqAq6qwHPSiS9+wNZwYlQR8Yk8pNvCKOOa6fordHUrN1atzGoFJfrYjgUaEIpPiFHf2eEwuq8jnA44+sB5BT+MJHoQQr9h9hswZq5RWmJLmmRNpuw1YrDOUsMCDD474IMBM4dkn9vrM85BagktoRvvB2hgRD/cIUR9ODRySfTe1i6DZFd8DAKolNpKI2S1c7UTPrFUcjeaLXVKUQ==
     procedure_id : 1613886137071329472
     type_code : 14
     code : |1+
      // Résumé : Actualise l'IHM selon la TVA
      // Syntaxe :
      // actualiseSelonTVA ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE actualiseSelonTVA()
      
      SELON gnModeCourant 
      
      	CAS MODE_TVA_UNIQUE 
      		SI ZM_TVA..Occurrence > 2 ALORS
      			zmlSupprime(ZM_TVA,3)
      		FIN
      		
      	CAS  MODE_TVA_SECONDAIRE
      		zmlAjouteLigne(ZM_TVA)
      		SI Devise.Symbole = "$" ALORS
      			ZM_TVA[3].SAI_TVA_SECONDAIRE..MasqueSaisie = "+99 999,99 $" + Devise.Symbole
      		SINON
      			ZM_TVA[3].SAI_TVA_SECONDAIRE..MasqueSaisie = "+99 999,99 " + Devise.Symbole
      		FIN
      		
      		
      		
      		
      FIN
      
     type : 458752
   -
     name : factureAffiche
     internal_properties : 
     procedure_id : 1613886137071395008
     type_code : 14
     code : |1-
      // Résumé : Affiche une note de frais
      // Syntaxe :
      //factureAffiche (<nIDFacture> est entier)
      //
      // Paramètres :
      //	nIDFacture (entier) :Identifiant de la note de frais
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE factureAffiche(LOCAL nIDFacture est un entier)
      
      // Se positionne sur la note de frais
      HLitRecherchePremier(Facture,IDFacture,nIDFacture)
      SI HTrouve(Facture) ALORS
      	
      	// RAZ
      	TableauSupprimeTout(gtabCollabo)
      	
      	// Affiche les informations
      	FichierVersEcran(FEN_FactureEdition_Tablette, Facture)
      	
      	// TVA unique ou deux TVA ?
      	SI Val(Facture.MontantTVA2) > 0  ALORS
      		gnModeCourant = MODE_TVA_SECONDAIRE
      		BTN_AJOUTE_TVA..ImageFond = IMG_MOINS
      		zmlAjouteLigne(ZM_TVA)	
      		ZM_TVA[3].SAI_TVA_SECONDAIRE = Facture.MontantTVA2
      		ZM_TVA[3].SAI_TAUX_TVA_SECONDAIRE = Facture.TauxTVA2
      	SINON
      		gnModeCourant = MODE_TVA_UNIQUE
      		BTN_AJOUTE_TVA..ImageFond = IMG_PLUS
      		SI ZM_TVA..Occurrence > 2 ALORS
      			zmlSupprime(ZM_TVA,3)
      		FIN
      		
      	FIN
      
      	// Affiche la devise
      	SetDevise(Facture.IDDevise)
      	
      	// Affiche la catégorie
      	SetCategorie(Facture.IDCatégorie)
      	
      	// Client
      	SetClient(Facture.IDClient)
      	
      	// Paiement
      	SetPaiement(Facture.IDTypePaiement)
      	
      	// Projet
      	SetProjet(Facture.IDProjet)
      	
      	// Statut
      	SetStatut(Facture.Statut)
      	
      	// Enveloppe
      	Setnote(Facture.IDNoteDeFrais)
      	
      	// Photo ?
      	TableauSupprimeTout(gtabPhotos)
      	TableauSupprimeTout(gtabSupprPhotos)
      	Hfiltre(Photos,IDFacture,Facture.IDFacture)
      	HLitPremier(Photos,IDFacture)
      	TANTQUE PAS HEnDehors(Photos)
      		stUnePhoto est un stPhoto
      		stUnePhoto.nID = Photos.IDPhotos
      		stUnePhoto.sChemin = Photos.Chemin
      		Ajoute(gtabPhotos,stUnePhoto)
      		
      		HLitSuivant(Photos,IDFacture)
      	FIN
      	HDésactiveFiltre(Photos)
      	
      	AfficheNbJustificatifs()
      	SI gtabPhotos..Occurrence > 0 ALORS
      		gnIndiceCourant = 1
      	SINON
      		gnIndiceCourant = 0	
      	FIN
      	
      	// Collaborateurs ?
      	HFiltre(Facture_Collaborateurs,IDFacture,Facture.IDFacture)
      	HLitPremier(Facture_Collaborateurs,IDFacture)
      	TANTQUE PAS HEnDehors(Facture_Collaborateurs)
      		Ajoute(gtabCollabo,Facture_Collaborateurs.IDCollaborateurs)
      		HLitSuivant(Facture_Collaborateurs)
      	FIN
      	HDésactiveFiltre(Facture_Collaborateurs)
      	
      	SetCollaborateur(gtabCollabo)
      	
      	// Possibilité de suppression
      	OPT_Supprimer..Visible = Vrai	
      		
      	
      	
      	sLibelléSpécial est une chaîne
      	nTypeSpécial est un entier
      	GetSpecialSelonCategorie(facture.IDCatégorie, sLibelléSpécial, nTypeSpécial)
      	
      	// Rubrique spéciale ?
      	SI sLibelléSpécial ~= "" ALORS
      		SetSpecial(CS_AUCUNE_VALEUR)
      	SINON
      		SetSpecial(Facture.IDCatégorie, Facture.ValeurSpéciale)	
      	FIN
      	actualiseHauteurCommentaire()
      FIN
     type : 458752
