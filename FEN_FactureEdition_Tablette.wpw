      // Résumé : Associe une note de frais à la facture
      // Syntaxe :
      //SetNote (<nIDNote>)
      //
      // Paramètres :
      //	nIDNote :ID Enveloppe
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE SetNote(LOCAL nIDNote)
      
      gnIDEnveloppe = nIDNote
      
      // Valeur existante ?
      SI nIDNote <> CS_AUCUNE_VALEUR ALORS
      	HLitRecherchePremier(NoteFrais,IDNoteDeFrais,nIDNote)
      	SI HTrouve(NoteFrais) ALORS
      		LIB_Note_de_frais = <§$0012§>+ NoteFrais.Nom
      	SINON
      		LIB_Note_de_frais = ""	
      	FIN
      SINON
      	LIB_Note_de_frais = ""
      FIN
