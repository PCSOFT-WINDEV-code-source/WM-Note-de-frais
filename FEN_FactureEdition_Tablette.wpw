      // Résumé : Positionne le projet
      // Syntaxe :
      //SetProjet (<nIDProjet>)
      //
      // Paramètres :
      //	nIDProjet :Identifiant du projet
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE SetProjet(LOCAL nIDProjet)
      
      gnIDProjet = nIDProjet
      
      // Valeur existante ?
      SI nIDProjet <> CS_AUCUNE_VALEUR ALORS
      	HLitRecherchePremier(Projet,IDProjet,nIDProjet)
      	SI HTrouve(Projet) ALORS
      		LIB_ChoixProjet = Projet.Nom
      	SINON
      		LIB_ChoixProjet= ""	
      	FIN
      SINON
      	
      	//Si 1 seul projet dans la base, on prend celui-là !
      	SI HNbEnr(projet) = 1 ALORS
      		HLitPremier(Projet)
      		LIB_ChoixProjet = Projet.Nom
      	SINON
      		LIB_ChoixProjet = ""
      	FIN
      	
      FIN
      
