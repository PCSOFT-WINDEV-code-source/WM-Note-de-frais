      			SetSpecial(CS_AUCUNE_VALEUR)
      		SINON
      			SetSpecial(nIDCategorie)	
      		FIN	
      	SINON
      		LIB_ChoixCatégorie = ""	
      	FIN
      SINON
      	LIB_ChoixCatégorie = ""
      FIN
      
