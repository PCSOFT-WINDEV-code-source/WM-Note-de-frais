         code : |1+
          
          // Variable
          dDateDebut est une Date = DateDuJour()
          dDateFin   est une Date = DateDuJour()
          
          // Saisie correcte ?
          SI SAI_DEMANDE_NOM ~= "" ALORS
          	Erreur(<§$0000§>)
          	RETOUR
          FIN
          
          ////Test retiré >> on laisse la possibilité de saisir des notes avec des noms identiques
          //// L'enveloppe n'existe t-elle pas déjà ?
          //HLitRecherchePremier(Enveloppe,Nom,SAI_DEMANDE_NOM)
          //SI HTrouve(Enveloppe) ALORS
          //	Erreur(<§$0002§>)
          //	RETOUR
          //FIN
          
          // Ajoute l'enveloppe
          HRAZ(NoteFrais)
          NoteFrais.Nom        = SAI_DEMANDE_NOM
          NoteFrais.DateDebut  = dDateDebut
          NoteFrais.DateFin    = dDateFin
          NoteFrais.AnnéeDebut = dDateDebut..Année
          NoteFrais.MoisDebut  = dDateDebut..Mois
          NoteFrais.AnnéeFin   = dDateFin..Année
          NoteFrais.MoisFin    = dDateFin..Mois
          NoteFrais.RefInterne = DonneIDNote()
          
          // Ajoute
          SI HAjoute(NoteFrais) = Vrai ALORS
          	
          	mafenêtre..ValeurRenvoyée = NoteFrais.IDNoteDeFrais
          	Ferme()
          	
          SINON
          	Erreur(HErreurInfo())
          FIN
          
